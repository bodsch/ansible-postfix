  {% endif %}
  {% if postfix_postscreen.bare_newline_enable is defined %}
postscreen_bare_newline_enable            = {{ postfix_postscreen.bare_newline_enable | bool | bodsch.core.config_bool(true_as='yes', false_as='no') }}
